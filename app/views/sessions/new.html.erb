<h1>Sign in, you hoser!</h1>
<%= form_for :signin, method: "POST" do |f| %>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <%= f.submit "Sign In" %>
<% end %>

